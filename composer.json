{
    "name": "your-username/opentimestamps-php",
    "description": "A complete PHP implementation of OpenTimestamps protocol",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "kornrunner/keccak": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "OpenTimestampsPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenTimestampsPHP\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/ots"]
}